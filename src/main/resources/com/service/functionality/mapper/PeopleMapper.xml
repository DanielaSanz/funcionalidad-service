<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.service.functionality.mapper.PeopleMapper">

    <select id="descriptionPeople" resultType="com.service.functionality.service.http.PeopleResponse">
        SELECT P.DESCRIPTION
        FROM PEOPLE P
        WHERE P.ID = 1
	</select>

</mapper>